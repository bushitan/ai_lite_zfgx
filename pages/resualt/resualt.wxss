/* pages/resualt/resualt.wxss */

.header{
    padding:  15px 15px 0;
}
.header .content{
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx #eee solid ;
    padding-bottom: 15px;
    font-size: 9pt;
    color: #ff726a;
}

.comment{
    padding: 15px;
}
.comment .node{
    display: flex;
    padding-bottom: 15px;
}
.comment .center{
    padding:0 0 0 15px;
}
.comment .logo{
    width: 46px;
    height: 46px;
}
.comment .title{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.comment .name{
    font-size: 10pt;
    color: #3784a8;
}
.comment .content{
    font-size: 11pt;
}
.comment .icon{
    width: 23px;
    height: 23px;
}
.comment label{
    font-size: 8pt;
}












































.xx_box{
    padding: 1px 15px 15px;
}
.xx_card{
    background-color: #fff;
    border-radius: 5px;
    box-shadow:0px 0px 10px #aaa;
}

.xx_image{
    display: flex;
    justify-content: center;
    position: relative;
    margin-top:30px;
    /* background-color: #eee; */
}
.org{
    /* position: absolute;
    top: 0; */
    width: 100%;
    /* height: 250px; */
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}
button{
    margin-top: 25px;
    /* font-size: 11pt; */
}

.sf{
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 13pt;
}
.sf image{
    width: 64px;
    height: 64px;
    border-radius: 50%;
    box-shadow:0px 0px 10px #ddd;
}
.sf label{
    margin-left:5px;
}