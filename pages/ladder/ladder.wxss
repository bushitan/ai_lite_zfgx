/* pages/ladder/ladder.wxss */
.des{
    font-size: 9pt;
    color: #888;
}
.ladder{
    padding: 10px 15px;
    display: flex;
    justify-content:space-between;
    align-items: center;
    background-color: #fff;
    margin-top: 5px;
}
.left{
    width: 100%;
    display: flex;
    align-items: center;
}
.num{ 
    font-size: 9pt;
    text-align: center;
    flex-basis: 60px; 
    overflow:hidden; 
    text-overflow:ellipsis; 
    white-space:nowrap; 
}
.name{ flex-basis: 150px;}
.logo{ flex-basis: 60px;}
image{
    width: 46px;
    height: 46px;
    border-radius: 50%;
}