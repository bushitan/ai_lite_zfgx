<ai-painter 
    bodyRectangle="{{bodyRectangle}}"
    landmark="{{landmark}}"
    catch:currentKey="currentKey"
    catch:currentIndex="currentIndex"
    move="{{move}}"
    bg="{{bg}}"
    pointColor="{{pointColor}}"
    lineColor="{{lineColor}}"
    getImage="{{getImage}}"
>
</ai-painter>


<!-- 位置选择 -->
<view class="section">
  <picker bindchange="bindPickerChange" range-key="name" range="{{keyArray}}" bindchange="pickKey">
    <view class="picker">
        <xx-navigte title="当前选择关节点：{{keyArray[keyIndex].name}}"></xx-navigte>
    </view>
  </picker>
</view>

<view class='control'>
    <image src='../../images/direction_top.png' catchtap='moveTop'></image>
    <view class='center'>
        <image src='../../images/direction_left.png' class='left' catchtap='moveLeft'></image>
        <label>微调</label>
        <image src='../../images/direction_right.png' catchtap='moveRight'></image>
    </view>
    <image src='../../images/direction_bottom.png' catchtap='moveBottom'></image>
</view>

<ai-make></ai-make>


<ai-btn
    catch:clickChoice="clickChoice"
    catch:clickDown="clickDown"
></ai-btn>












